extrae_homicidios_fecha <- function(datos, anio)
{
datos <- datos %>%
select(Año, Entidad,`Tipo de delito`, Modalidad, Total) %>%
filter(Año == anio)
return(datos)
}
# bucle
for (i in 2015:2018)
{
homicidios_anio <- extrae_homicidios_fecha(datos, i)
homicidios_fecha <- toString(i)
